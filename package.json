{
  "name": "projetIntegrateur",
  "version": "0.0.1",
  "scripts": {
    "build": "npx ts-node ./scripts/build.ts",
    "dev": "nodemon",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src -e ts",
    "lint": "npx eslint ./src",
    "lint:tests": "npx eslint ./spec",
    "start": "cross-env NODE_ENV=production node -r ./preload.js ./dist",
    "test": "npx ts-node ./spec",
    "test:hot": "nodemon --exec \"npm run test\" --watch ./src --watch ./spec -e ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r ./src"
  },
  "engines": {
    "node": ">=16.6.0"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.7",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^8.0.0",
    "inserturlparams": "^2.0.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.0.9",
    "jet-validator": "^1.1.1",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "mock-mongoose": "^8.0.0",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "mongoose": "^8.8.2",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-schema-official": "^2.0.0-bab6bed",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@jazim/mock-mongoose": "^1.0.1",
    "@stylistic/eslint-plugin-ts": "^2.11.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.4",
    "@types/jsonfile": "^6.1.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.1",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/yamljs": "^0.2.34",
    "bcrypt": "^5.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.15.0",
    "eslint-plugin-n": "^17.14.0",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jasmine": "^5.4.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0"
  },
  "_moduleAliases": {
    "@src": "./dist"
  }
}
